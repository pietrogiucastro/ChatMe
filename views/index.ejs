    .chat-tab .volume-icon {
      position: absolute;
      left: 2px;
      top: 100%;
      padding: 1px;
      padding-top: 4px;
    }
    .vol-true {
      color: green;
    }
    .vol-false {
      color: darkred;
    }
    .volume-icon.vol-true:before {
      font-family: fontawesome;
      content: "\f028";
    }
    .volume-icon.vol-false:before {
      font-family: fontawesome;
      content: "\f026";
    }
    .chat-tab:before {
      position: absolute;
      font-family: fontawesome;
      font-size: 11px;
      left: 5px;
      padding-top: 6px;
      top: 0%;
    }
    .chat-tab.global-tab:before {
      content: "\f0ac";
    }
    .chat-tab.site-tab:before {
      content: "\f0ac";
    }
    .chat-tab.pass:before {
      content: "\f023";
    }
    .chat-tab.free:before {
      content: "\f0ac";
    }
    .chat-tab.custom {

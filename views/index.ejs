    .cm-tabs-scroll::-webkit-scrollbar-track
      {
        --webkit-box-shadow: none !important;
        background-color: #37617c !important;
      }
  </style>
  <style type="text/css">
    .chat-tab:hover .volume-icon {
      animation: upin 0.13s forwards;
    }
    .chat-tab:hover:before {
      animation: upout 0.13s forwards;
    }
    .chat-tab .volume-icon {
      animation: downout 0.13s forwards;
      width: 13px;
    }

    .chat-tab:before {
      top: -100%;
      animation: downin 0.13s forwards;
    }

    .cm-seen {
      animation: seen 1.20s forwards;
    }

    #cm-record.recording {
      animation: btnrec 1.2s infinite;
      animation-direction: alternate;
    }

    #cm-record.recording .micico {
      animation: downout 0.4s forwards;
    }

    #cm-record.recording .rec-btns {
      animation: upin 0.3s forwards;
    }

    #cm-record:not(.recording).stopped .micico {
      animation: upin 0.3s forwards;
    }

    #cm-record:not(.recording).stopped .rec-btns {
      animation: downout 0.3s forwards;
    }

    .username.user-recording:after {
      animation: userrec 0.9s infinite;
      animation-direction: alternate;
    }

    .recording .record-time:before {
      animation: userrec 0.9s infinite;
      animation-direction: alternate;
    }

    @keyframes upin {
      from {
        top: 100%;
      }
      to {
        top: 0%;
      }
    }

    @keyframes downout {
      from {
        top: 0%;
      }
      to {
        top: 100%;
      }
    }

    @keyframes downin {
      from {
        top: -100%;
      }
      to {
        top: 0%;
      }
    }

    @keyframes upout {
      from {
        top: 0%;
      }
      to {
        top: -100%;
      }
    }

    @keyframes seen {
      from {
        background-color: #cce2cc;
      }
      to {
        background-color: transparent;
      }
    }

    @keyframes btnrec {
      from {
        background-color: #c9813f;
      }
      to {
        background-color: #a26a38;
      }
    }

    @keyframes userrec {
      from {
        opacity: 0.0;
      }
      to {
        opacity: 1.0;
      }
    }
  </style>

  <style type="text/css">
    .audio-track {
      -webkit-appearance: none; /* Hides the slider so that custom slider can be made */
      width: 100%;
      height: 100%;
      margin: 0;
      background: transparent; /* Otherwise white in Chrome */
    }

    .audio-track::-webkit-slider-thumb {
      -webkit-appearance: none;
    }

    .audio-track:focus {
      outline: none;
    }

    .audio-track::-ms-track {
      width: 100%;
      cursor: pointer;

      /* Hides the slider so custom styles can be added */
      background: transparent; 
      border-color: transparent;
      color: transparent;
    }



    /* Special styling for WebKit/Blink */
    .audio-track::-webkit-slider-thumb {
      -webkit-appearance: none;
      height: 12px;
      width: 12px;
      border-radius: 50%;
      background: rgb(70,130,210);
      cursor: pointer;
      margin-top: -5px;
    }

    /* All the same stuff for Firefox */
    .audio-track::-moz-range-thumb {
      -webkit-appearance: none;
      height: 14px;
      width: 14px;
      border-radius: 50%;
      background: rgb(70,130,210);
      cursor: pointer;
    }

    /* All the same stuff for IE */
    .audio-track::-ms-thumb {
      -webkit-appearance: none;
      height: 14px;
      width: 14px;
      border-radius: 50%;
      background: rgb(70,130,210);
      cursor: pointer;
    }

    .audio-track:active::-webkit-slider-thumb {
      width: 18px;
      height: 18px;
      margin-top: -8px;
    }

    .user-You .audio-track::-webkit-slider-thumb,
    .user-You .audio-track::-moz-range-thumb,
    .user-You .audio-track::-ms-thumb {
      background: rgb(70,210,90) !important;
    }


    .audio-track::-webkit-slider-runnable-track {
      width: 100%;
      height: 2px;
      cursor: pointer;
      background: #3071a9;
      border-radius: 3px;
      background-color: rgba(85,85,85,0.6);
    }

    .audio-track::-moz-range-track {
      width: 100%;
      height: 2px;
      cursor: pointer;
      box-shadow: 1px 1px 1px #000000, 0px 0px 1px #0d0d0d;
      background: #3071a9;
      border-radius: 1.3px;
      border: 0.2px solid #010101;
    }

    .audio-track::-ms-track {
      width: 100%;
      height: 8.4px;
      cursor: pointer;
      background: transparent;
      border-color: transparent;
      border-width: 16px 0;
      color: transparent;
    }
    .audio-track::-ms-fill-lower {

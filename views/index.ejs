  <style type="text/css">
    .cm-scroll {
      overflow-y: auto;
    }
    .cm-scroll.scroll-x {
      overflow-y: hidden;
      overflow-x: auto;
    }
    .cm-scroll::-webkit-scrollbar-track
      {
        -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
        background-color: #F5F5F5;
      }

    .cm-scroll::-webkit-scrollbar
      {
